{
  "name": "react-optimizely",
  "version": "1.0.1",
  "description": "React helpers for the Optimizely JS API",
  "main": "dist/index.js",
  "repository": "https://github.com/onbjerg/react-optimizely.git",
  "author": "Oliver Nordbjerg <hi@notbjerg.me>",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.21.0",
    "babel-cli": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "nyc": "^11.0.1",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-test-render": "^1.0.3",
    "standard": "^14.0.0"
  },
  "peerDependencies": {},
  "scripts": {
    "test": "standard && nyc --reporter=lcov ava",
    "test:report": "nyc report",
    "prepublish": "npm run build",
    "build": "babel -d dist src"
  },
  "ava": {
    "concurrency": 5,
    "require": [
      "babel-register"
    ],
    "files": [
      "test/*.js",
      "!test/util.js"
    ],
    "babel": "inherit"
  }
}
